<routes>
    <page type="Zend_Controller_Router_Route">
        <route>pages/:controller/:action/*</route>
        <defaults module="pages" controller="index" action="index" />
    </page>
    <pages-view type="Zend_Controller_Router_Route">
        <route>page/:alias/*</route>
        <defaults module="pages" controller="index" action="view" />
    </pages-view>
</routes>